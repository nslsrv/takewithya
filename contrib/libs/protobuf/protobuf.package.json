{
    "meta": {
        "name": "yandex-contrib-protobuf-linux",
        "maintainer": "devtools <devtools@yandex-team.ru>",
        "description": "yandex-contrib-protobuf-linux",
        "version": "{revision}"
    },
    "build": {
        "targets": [
            "contrib/libs/protobuf"
        ],
        "flags": [
            {
                "name": "CFLAGS",
                "value": "-fPIC"
            }
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "contrib/libs/protobuf/libcontrib-libs-protobuf.a"
            },
            "destination": {
                "path": "/local/linux.x86-64/lib/libcontrib-protobuf-full.a"
            }
        }
    ]
}
