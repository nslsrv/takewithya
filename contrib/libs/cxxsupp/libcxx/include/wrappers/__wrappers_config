#ifndef _LIBCPP_WRAPPERS_CONFIG_H
#define _LIBCPP_WRAPPERS_CONFIG_H

#define _LIBCPP_POINTER_VECTOR_ITERATORS 1

#ifdef __cplusplus
#include <../__config>
#endif

#ifdef _LIBCPP_MSVC
#   define _LIBCXX_NATIVE_HEADER(HEADER) <../../VC/include/HEADER>
#else
#   define _LIBCXX_NATIVE_HEADER(HEADER) <native_headers_not_supported_on_this_platform>
#endif

#define _LIBCXX_HEADER(HEADER) <../HEADER>

#define _LIBCXX_CAT(a, b) _LIBCXX_CAT_I(a, b)
#define _LIBCXX_CAT_I(a, b) a ## b

#define _LIBCXX_PROPER_HEADER(HEADER) _LIBCXX_CAT(_LIBCXX_PROPER_HEADER_HELPER_, _LIBCXX_USE_NATIVE_STL)(HEADER)
#define _LIBCXX_PROPER_HEADER_HELPER__LIBCXX_USE_NATIVE_STL(HEADER) _LIBCXX_HEADER(HEADER)
#define _LIBCXX_PROPER_HEADER_HELPER_0(HEADER) _LIBCXX_HEADER(HEADER)
#define _LIBCXX_PROPER_HEADER_HELPER_1(HEADER) _LIBCXX_NATIVE_HEADER(HEADER)

#endif // _LIBCPP_WRAPPERS_CONFIG_H
